/etc/network/interfaces
auto io iface il inet loopback
oallow-hotplug enp0s3
auto enp0s3
iface enp0s3 inet static
address 192.168.1.10
netmask 255.255.255.0
network 192.168.1.0
broadcast 192.168.1.255 
gateway 192.168.1.1



/etc/resolv.conf
domain localdomain
search localdomain
nameserver 8.8.8.8

